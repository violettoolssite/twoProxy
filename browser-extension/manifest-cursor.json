{
  "manifest_version": 3,
  "name": "YLJD Cursor 自动填写助手",
  "version": "1.0.0",
  "description": "自动填写 Cursor 注册表单和验证码",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://authenticator.cursor.sh/*",
    "https://*.cursor.sh/*",
    "https://*.cursor.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://authenticator.cursor.sh/*",
        "https://*.cursor.sh/*",
        "https://*.cursor.com/*"
      ],
      "js": ["cursor-auto-fill.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}

